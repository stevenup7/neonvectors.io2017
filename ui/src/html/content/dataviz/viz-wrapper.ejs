
<script src="https://cdn.rawgit.com/google/code-prettify/master/loader/run_prettify.js"></script>
<% vizData.pages[0].libs.forEach(function(script) { %>
   <script src="/static/js/<%- script %>"></script>
<% }) %>


<div class="container" id="vc" v-html="">
  vc: {{counter}}

  <h1 class="title"><%- vizData.pages[0].title %></h1>
  <section id="viz-page-container" class="top-padded columns">
      <%- include(vizTemplate) %>
  </section>

</div>

<script>
  var pageData = <%- JSON.stringify(vizData, null, '  ') %>;
</script>
<script src="/static/js/dataviz.js"></script>
